<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="BOTTOM_RIGHT" prefHeight="50.0" prefWidth="400.0" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfx.SleepPopupController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="20.0" prefWidth="200.0" VBox.vgrow="NEVER">
         <children>
            <Label text="Time in milliseconds: " />
            <TextField fx:id="entryBox" onAction="#ok" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#ok" text="OK" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
